{
    "name": "mk-koa",
    "version": "1.0.0",
    "description": "Koa + ts + ...基础模板",
    "main": "main.js",
    "type": "commonjs",
    "warName": "koa",
    "homepage": "https://mankeung.github.io/docs/mk-koa",
    "repository": {
        "type": "git",
        "url": "https://github.com/mankeung/mk-koa"
    },
    "scripts": {
        "dev": "nodemon",
        "build": "rollup --config rollup.config.ts --configPlugin typescript",
        "start": "pm2 start pm2.json",
        "restart": "pm2 restart pm2.json",
        "stop": "pm2 stop pm2.json",
        "delete": "pm2 delete pm2.json",
        "lint:eslint": "eslint ./src/**/*.ts --fix",
        "lint:perttier": "prettier --write --loglevel warn src/**/*.ts",
        "lint": "pnpm lint:perttier && pnpm lint:eslint",
        "prepare": "node ./scripts/prepare.js",
        "precommit": "node ./scripts/precommit.js",
        "war": "node ./scripts/war.js"
    },
    "keywords": [
        "koa",
        "ts",
        "node",
        "rollup"
    ],
    "author": "mankeung",
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@rollup/plugin-commonjs": "^23.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-typescript": "^9.0.1",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/koa": "^2.13.5",
        "@types/koa-bodyparser": "^4.3.8",
        "@types/koa-compress": "^4.0.3",
        "@types/koa-mount": "^4.0.1",
        "@types/koa-static": "^4.0.2",
        "@types/koa2-cors": "^2.0.2",
        "@types/koa__router": "^12.0.0",
        "@types/node": "^18.8.5",
        "@types/node-rsa": "^1.1.1",
        "@types/rollup-plugin-json": "^3.0.3",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "enquirer": "^2.3.6",
        "eslint": "^8.25.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-promise": "^6.1.0",
        "folder-zip-sync": "^1.0.5",
        "husky": "^8.0.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "rollup": "^3.1.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-clear": "^2.0.7",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-eslint2": "^1.0.2",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@koa/router": "^12.0.0",
        "chalk": "4",
        "file-type": "16",
        "jsonwebtoken": "^8.5.1",
        "koa": "^2.13.4",
        "koa-body": "^5.0.0",
        "koa-compress": "^5.1.0",
        "koa-helmet": "^6.1.0",
        "koa-jsonp": "^2.0.2",
        "koa-mount": "^4.0.0",
        "koa-static": "^5.0.0",
        "koa2-cors": "^2.0.6",
        "log-symbols": "4",
        "log4js": "^6.7.0",
        "moment": "^2.29.4",
        "node-rsa": "^1.1.1",
        "uuid": "^9.0.0"
    }
}
